server {
   server_name ec2-18-188-66-127.us-east-2.compute.amazonaws.com;
   root /home/ubuntu/szinti/public;
   index index.html index.htm;
   location /static {
   proxy_pass  http://localhost:8080;
   }
   location /api {
   proxy_pass  http://localhost:8080;
   }
   location / {
   try_files $uri /index.html =404;
   }
}